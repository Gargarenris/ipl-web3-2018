{
  "name": "web3-boilerplate-2018-2019",
  "productName": "Electron Project",
  "version": "1.0.0",
  "main": "src/server/bin/www",
  "private": true,
  "scripts": {
    "start": "node src/server/bin/www",
    "build": "webpack --mode production --config config/webpack.config.js",
    "clean": "rm -rf dist",
    "client": "webpack-dev-server --mode development --config config/webpack.config.js",
    "server": "nodemon src/server/bin/www",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "npm run build",
    "electron-webbundle": "webpack --mode production --config config/webpack-electron.config.js",
    "electron-builder": "cross-env \"npm run electron-webbundle && npm run package-win\"",
    "electron-dev": "cross-env \"npm run electron-webbundle && electron ./src/electron/main.js \"",
    "package-mac": "electron-packager ./src/electron/ Web3versionElectron --overwrite --platform=darwin --arch=x64  --prune=true --out=release-builds",
    "package-win": "electron-packager ./src/electron/ Web3versionElectron --overwrite --asar=true --platform=win32 --arch=ia32  --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Web3versionElectron\"",
    "package-linux": "electron-packager ./src/electron/ Web3versionElectron --overwrite --asar=true --platform=linux --arch=x64  --prune=true --out=release-builds"
  },
  "dependencies": { 
    "bcrypt": "^3.0.2",
    "bootstrap": "^4.1.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "ejs": "~2.5.7",
    "electron-packager": "^12.2.0",
    "express": "^4.16.4",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.4.0",
    "mongodb": "^3.1.6",
    "morgan": "~1.9.0",
    "node-sass": "^4.10.0",
    "node-sass-middleware": "0.11.0",
    "react": "^16.3.3",
    "react-bootstrap": "^1.0.0-beta.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.4.7",
    "sass-loader": "^7.1.0",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "electron": "^3.0.9",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.6",
    "style-loader": "^0.22.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.10"
  }
}
